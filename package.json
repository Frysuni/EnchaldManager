{
  "name": "echnald-manager",
  "author": "Frys",
  "private": true,
  "license": "USER_AGREEMENT.TXT",
  "engines": {
    "node": ">=16.0.0 <19.0.0"
  },
  "scripts": {
    "boot": "npm i -g yarn@latest && yarn install --check-files --prod && npm run build && npm run schema:sync && npm run pm2",
    "pm2": "npm i -g pm2@latest && pm2 update && pm2 start ecosystem.config.js -i 4 && pm2 startup && pm2 save",
    "build": "nest build -p tsconfig.build.json",
    "dev": "cross-env NODE_ENV=dev NODE_OPTIONS=--trace-warnings nest start --watch --debug 9229 --preserveWatchOutput -p tsconfig.dev.json",
    "start": "cross-env NODE_ENV=production node ./dist/main.js",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "schema:sync": "yarn run typeorm -d ./dist/ormconfig.js schema:sync",
    "schema:drop": "yarn run typeorm -d ./dist/ormconfig.js schema:drop"
  },
  "dependencies": {
    "@discord-nestjs/common": "^5.2.6",
    "@discord-nestjs/core": "^5.3.8",
    "@nestjs/cli": "^10.1.10",
    "@nestjs/common": "^10.1.0",
    "@nestjs/core": "^10.1.0",
    "@nestjs/platform-socket.io": "^10.1.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.1.0",
    "cron": "^2.3.1",
    "cron-parser": "^4.8.1",
    "cross-env": "^7.0.3",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "env-var": "^7.3.1",
    "minecraft-server-util": "^5.4.4",
    "mysql2": "^3.5.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@types/cron": "^2.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  }
}
